    "gatsby-source-filesystem": "~3.5.0",
    "prop-types": "~15.7.2",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "typescript": "~4.0.3"
  },
  "devDependencies": {
    "eslint": "~7.11.0",
    "eslint-config-airbnb": "~18.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "~6.4.1",
